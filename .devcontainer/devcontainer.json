{
	"name": "Terraform",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "tf",
	"workspaceFolder": "/home",
	"initializeCommand": "aws-vault exec admin_role -- env | grep AWS > .aws.env",
	"settings": {
		"terminal.integrated.defaultProfile.linux": "/bin/bash",
		"editor.formatOnSave": true
	},
	"extensions": [
		"hashicorp.terraform"
	]
}
